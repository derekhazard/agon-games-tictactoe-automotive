<?xml version="1.0" encoding="utf-8"?>
<!--
    Launcher icon foreground — 3×3 tic-tac-toe grid with X marks on the
    main diagonal (top-left, centre, bottom-right).

    Sizing rationale:
      The AAOS launcher clips the adaptive icon to the safe-zone circle:
      radius 36 centred at (54,54) in the 108×108 viewport.
      For the board's four corners to just touch that circle:
        side = 36 × √2 ≈ 50.9 dp  →  board (29,29)→(79,79)
        corner distance from centre = 25√2 ≈ 35.4 dp  ✓ inside radius 36

    Grid (each cell ≈16–17 dp):
      columns/rows: 29  46  62  79
    Grid dividers at x=46, x=62 (vertical) and y=46, y=62 (horizontal).

    X marks (3 dp padding inside each cell, strokeWidth 5):
      cell (0,0)  29–46 / 29–46  →  arms (32,32)↔(43,43)  (43,32)↔(32,43)
      cell (1,1)  46–62 / 46–62  →  arms (49,49)↔(59,59)  (59,49)↔(49,59)
      cell (2,2)  62–79 / 62–79  →  arms (65,65)↔(76,76)  (76,65)↔(65,76)
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Grid dividers — subtle border colour -->
    <path
        android:pathData="M46,29 L46,79"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M62,29 L62,79"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,46 L79,46"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,62 L79,62"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (0,0) top-left -->
    <path
        android:pathData="M32,32 L43,43"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M43,32 L32,43"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (1,1) centre -->
    <path
        android:pathData="M49,49 L59,59"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M59,49 L49,59"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (2,2) bottom-right -->
    <path
        android:pathData="M65,65 L76,76"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M76,65 L65,76"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

</vector>
